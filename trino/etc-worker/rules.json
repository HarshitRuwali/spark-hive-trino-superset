{
  "catalogs": [
    {
      "user": "p2p",
      "catalog": "hive",
      "allow": "all"
    }
  ],
  "schemas": [
    {
      "user": "p2p",
      "catalog": "hive",
      "schema": ".*",
      "owner": true
    }
  ],
  "tables": [
    {
      "user": "p2p",
      "catalog": "hive",
      "schema": ".*",
      "table": ".*",
      "privileges": ["SELECT", "INSERT", "UPDATE", "DELETE", "GRANT_SELECT", "OWNERSHIP"]
    },
    {
      "user": "p2p",
      "catalog": "system",
      "schema": ".*",
      "table": ".*",
      "privileges": ["SELECT"]
    }
  ],
  "queries": [
    {
      "user": "p2p",
      "allow": ["execute", "kill", "view"]
    }
  ]
}